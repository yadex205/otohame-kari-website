    <!-- <section class="t-default__section"><h2 class="a-heading">遊び方</h2></section> -->

    <section class="t-default__section">
      <h2 class="a-heading">頒布概要</h2>
      <div class="p-top-distinfo">
        <div class="p-top-distinfo__item">2018.08.10 金</div>
        <div class="p-top-distinfo__item">
          <small>コミックマーケット94</small>
          <div>西地区 "る"-33b</div>
        </div>
        <div class="p-top-distinfo__item">
          <small>サークル OY旅団 (OY Brigade)</small>
        </div>
      </div>
    </section>

    <section class="t-default__section t-default__section--wrapped">
      <h3 class="a-heading">クレジット</h3>
      <div class="p-top-credit">
        <div class="p-top-credit__logo-wrapper">
          <img class="p-top-credit__logo" alt="OY Brigade" src="./assets/img/oy_brigade_logo.png" />
        </div>
        <ul class="p-top-credit__team">
          <li class="p-top-credit__team-member">
            <img class="p-top-credit__team-member-icon" alt="" src="" />
            <div class="p-top-credit__team-member-role">Illust</div>
            <div class="p-top-credit__team-member-name">がんたん</div>
          </li>
          <li class="p-top-credit__team-member">
