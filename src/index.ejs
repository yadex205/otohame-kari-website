            <div class="p-top-credit__team-member-role">Illust</div>
            <div class="p-top-credit__team-member-name">くれいじー</div>
          </li>
          <li class="p-top-credit__team-member">
            <img class="p-top-credit__team-member-icon" alt="" src="" />
            <div class="p-top-credit__team-member-role">Program</div>
            <div class="p-top-credit__team-member-name">パフーハ</div>
          </li>
          <li class="p-top-credit__team-member">
            <img class="p-top-credit__team-member-icon" alt="" src="" />
            <div class="p-top-credit__team-member-role">Sound</div>
            <div class="p-top-credit__team-member-name">Nasubeing</div>
          </li>
          <li class="p-top-credit__team-member">
            <img class="p-top-credit__team-member-icon" alt="" src="" />
            <div class="p-top-credit__team-member-role">Web</div>
            <div class="p-top-credit__team-member-name">Yadex205</div>
          </li>
        </ul>
      </div>
    </section>

    <%- include('/src/components/organisms/footer/_footer') %>
