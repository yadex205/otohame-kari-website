    <link href="./assets/css/app.css" rel="stylesheet" />
  </head>
  <body class="t-default p-top">
    <section class="t-default__section t-default__section--cover">
      <%- include('/src/components/organisms/mainvisual/_mainvisual') %>
    </section>

    <section class="t-default__section t-default__section--full-width p-top-intro">
      <div class="p-top-intro__text-wrapper">
        <p class="p-top-intro__text">
          ダウンロードは
          <a href="https://drive.google.com/drive/folders/1DgLNLCHLeYYQ7UzBnmNxeKlCs8G3MCa-?usp=sharing"
